<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.maoding.corpbll.module.corpserver.dao.SyncCompanyDao">
    <select id="selectSyncCompany" resultType="com.maoding.corpbll.module.corpserver.dto.SyncCompanyDto_Select">
        SELECT s.*,c.company_name
        FROM maoding_corp_sync_company s
        LEFT JOIN maoding_web_company c ON c.id = s.company_id
        <where>
            <if test="corpEndpoint !=null">
                AND s.corp_endpoint= #{corpEndpoint}
            </if>
        </where>
        ORDER BY s.corp_endpoint,s.company_id
    </select>
</mapper>